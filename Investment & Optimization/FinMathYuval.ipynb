    "from cvxopt import matrix, solvers\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "#markovitz function for finding the optimal with givin cov matrix\n",
    "#help(solvers.qp)\n",
    "\n",
    "def markowitz(mu, cov_matrix):\n",
    "\n",
    "    cov_matrix= np.array(cov_matrix)\n",
    "    Constrains_left=np.array([np.array(avg_m),[1,1]],dtype=np.float64)\n",
    "    Constrains_right=np.array([[1,mu]],dtype=np.float64).T\n",
    "    P=matrix(cov_matrix) #From numpy to CVXOPT\n",
    "    A=matrix(Constrains_left) #From numpy to CVXOPT\n",
    "    b=matrix(Constrains_right) #From numpy to CVXOPT\n",
    "    q=matrix([0.0,0.0]) \n",
    "    #using solver for optimization\n",
    "    solution=solvers.qp(P,q,A=A,b=b)\n",
    "    return np.array(solution['x']).T[0]\n",
    "def markowitzmvp(mu, cov_matrix):\n",
    "\n",
    "    cov_matrix= np.array(cov_matrix)\n",
    "    Constrains_left=np.array([[1,1]],dtype=np.float64)\n",
    "    Constrains_right=np.array([[1]],dtype=np.float64).T\n",
    "    P=matrix(cov_matrix) #From numpy to CVXOPT\n",
    "    A=matrix(Constrains_left) #From numpy to CVXOPT\n",
    "    b=matrix(Constrains_right) #From numpy to CVXOPT\n",
    "    q=matrix([0.0,0.0]) \n",
    "    #using solver for optimization\n",
    "    solution=solvers.qp(P,q,A=A,b=b)\n",
    "    return np.array(solution['x']).T[0]\n",
    "\n",
    "def sigma_value(mu):\n",
    "    w=markowitz(mu, cov_matrix)\n",
    "    w=w.reshape(1,-1)\n",
    "    return(np.sqrt(w.dot(cov_matrix).dot(w.T)))\n",
    "\n",
    "\n",
    "df=pd.read_csv('stock_data.csv')\n",
    "df=df.set_index(pd.to_datetime(df.Date))\n",
    "#8 stocks\n",
    "df=df[['GLD', 'SPY']]\n",
    "#organizing data by weeks\n",
    "df=df.resample('W').last()\n",
    "#calculating precentage\n",
    "pct_change=df.pct_change()[1:]\n",
    "\n",
    "cov_matrix=pct_change.cov()\n",
    "\n",
    "avg_m = pct_change.mean()\n",
    "sigma_list=[]\n",
    "sigma_list1=[]\n",
    "sigma_list2=[]\n",
    "sigma_list3=[]\n",
    "\n",
    "a=np.linspace(np.amin(avg_m),np.amax(avg_m)*2,100)\n",
    "\n",
    "for i in a:\n",
    "    w=markowitz(i,cov_matrix)\n",
    "    w=w.reshape(1,-1)\n",
    "    sigma_list.append(np.sqrt(w.dot(cov_matrix).dot(w.T)))\n",
    "    if np.sqrt(w.dot(cov_matrix).dot(w.T)) > np.amin(sigma_list):\n",
    "        sigma_list1.append(np.sqrt(w.dot(cov_matrix).dot(w.T)))\n",
    "        \n",
    "w=markowitzmvp(1,cov_matrix)\n",
    "w=w.reshape(1,-1)\n",
    "sigma_list2.append(np.sqrt(w.dot(cov_matrix).dot(w.T)))\n",
    "\n",
    "    \n",
    "plt.scatter(sigma_list,a, color = 'green') #rest of options\n",
    "plt.scatter(sigma_list1,a[len(sigma_list1):], color = 'blue') #more efficient values\n",
    "plt.scatter(sigma_list2,0, color = 'red') #mvp\n",
    "\n",
    "plt.show()\n",
   ]
